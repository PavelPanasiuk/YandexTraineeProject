pipeline {
    agent any
     stages {
        stage('Test') {
            steps {
                echo 'dotnet test '
            }

    }

        stage('Reports') {
                allure([
                    includeProperties: false,
                    jdk: '',
                    properties: [],
                    reportBuildPolicy: 'ALWAYS',
                    results: [[path: 'target/allure-results']]
                ])
            }
    }
    
}

